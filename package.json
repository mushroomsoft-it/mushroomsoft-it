{
  "name": "mushroomsoft-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /mushroomsoft-ws/",
    "build:lib": "ng build lib",
    "test": "ng test --codeCoverage=true --watch=false",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint -c .eslintrc.json --ext '.js,.ts' --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier -- -l",
    "prettier:cli": "npm run prettier -- -c",
    "prettier:fix": "npm run prettier:cli -- --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ngx-formly/bootstrap": "^5.12.3",
    "@ngx-formly/core": "^5.12.3",
    "@sendgrid/mail": "^7.7.0",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.1",
    "primeicons": "^5.0.0",
    "primeng": "^13.3.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "url-join": "^5.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.4",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.4",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.2",
    "typescript": "~4.6.2"
  },
  "author": {
    "name": "Mushroomsoft"
  }
}
